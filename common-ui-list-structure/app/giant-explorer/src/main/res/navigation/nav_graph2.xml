<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph2"
    app:startDestination="@id/RootAccessStatusFragment">

    <fragment
        android:id="@+id/RootAccessStatusFragment"
        android:name="com.storyteller_f.giant_explorer.control.RootAccessStatusFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_root_access_status">

        <action
            android:id="@+id/action_RootAccessStatusFragment_to_Second2Fragment"
            app:destination="@id/Second2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/Second2Fragment"
        android:name="com.storyteller_f.giant_explorer.control.Second2Fragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second2">

        <action
            android:id="@+id/action_Second2Fragment_to_First2Fragment"
            app:destination="@id/RootAccessStatusFragment" />
    </fragment>
</navigation>